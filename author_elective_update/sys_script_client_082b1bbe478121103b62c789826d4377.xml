<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Only display What needed choices that match the Request type value.</description>
        <field>u_request_type</field>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name>NeedIt Request Type Options</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if ( newValue == '') {
      return;
    }
    
	// Változó, ami beolvassa a What needed értékét
    var whatneeded = g_form.getValue('u_what_needed');
    
    // Törli az összes lehetőséget a What needed alól, hogy majd csak a megfelelőeket adjuk vissza neki
    g_form.clearOptions('u_what_needed');
    
    // Ha az érték HR, akkor a HR releváns dolgokat adja vissza + az egyéb lehetősget
	// A szükséges értékeket meg tudjuk nézni pl. a New record alatt a What needed-re jobb gombbal kattintva a Show choice list alatt
    if(newValue == 'hr'){
      g_form.addOption('u_what_needed','hr1','Human Resources 1');
      g_form.addOption('u_what_needed','hr2','Human Resources 2');
      g_form.addOption('u_what_needed','other','Other');
    }
       // Ha az érték Facilities, akkor a facility releváns dolgokat adja vissza + az egyéb lehetősget
    if(newValue == 'facilities'){
      g_form.addOption('u_what_needed','facilities1','Facilities 1');
      g_form.addOption('u_what_needed','facilities2','Facilities 2');
      g_form.addOption('u_what_needed','other','Other');
    }
       // Ha az érték legal, akkor a legal releváns dolgokat adja vissza + az egyéb lehetősget
    if(newValue == 'legal'){
      g_form.addOption('u_what_needed','legal1','Legal 1');
      g_form.addOption('u_what_needed','legal2','Legal 2');
      g_form.addOption('u_what_needed','other','Other');
    }
    // Ha a form betölt és nem új rekord, akkor az u_what_needed értéke az maradjon ami a betöltés előtt volt
    
    if(isLoading && !g_form.isNewRecord()){
      g_form.setValue('u_what_needed', whatneeded);
    }
  }]]></script>
